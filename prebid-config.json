{
  "version": "10.18.0",
  "modules": [
    "msftBidAdapter",
    "appnexusBidAdapter",
    "pubmaticBidAdapter",
    "rubiconBidAdapter",
    "consentManagementGpp",
    "consentManagementTcf",
    "storageControl",
    "tcfControl",
    "schain",
    "id5IdSystem",
    "pairIdSystem",
    "sharedIdSystem",
    "unifiedIdSystem"
  ],
  "globalConfig": {
    "debug": true,
    "bidderTimeout": 10000,
    "priceGranularity": "auto",
    "enableSendAllBids": true,
    "useBidCache": true
  },
  "consentManagement": {
    "gdpr": {
      "cmpApi": "iab",
      "timeout": 5000,
      "defaultGdprScope": true,
      "allowAuctionWithoutConsent": false,
      "usp": {
        "cmpApi": "iab",
        "timeout": 1000
      }
    },
    "usp": {
      "cmpApi": "iab",
      "timeout": 1000
    }
  },
  "userSync": {
    "auctionDelay": 0,
    "syncDelay": 1000,
    "syncEnabled": true,
    "syncsPerBidder": 5,
    "filterSettings": {
      "iframe": {
        "bidders": "*",
        "filter": "include"
      },
      "image": {
        "bidders": "*",
        "filter": "include"
      }
    },
    "userIds": [
      {
        "name": "sharedId",
        "storage": {
          "expires": 60,
          "name": "_sharedid",
          "type": "cookie"
        }
      },
      {
        "name": "criteo"
      },
      {
        "name": "id5Id",
        "params": {
          "partner": 42342343
        },
        "storage": {
          "type": "html5",
          "name": "id5id",
          "expires": 90,
          "refreshInSeconds": 28800
        }
      },
      {
        "name": "unifiedId",
        "params": {
          "url": "//match.adsrvr.org/track/rid?ttd_pid=az&fmt=json"
        },
        "storage": {
          "type": "cookie",
          "name": "pbjs-unifiedid",
          "expires": 60
        }
      }
    ]
  },
  "schain": {
    "validation": "relaxed",
    "config": {
      "ver": "1.0",
      "complete": 1,
      "nodes": [
        {
          "asi": "your-domain.com",
